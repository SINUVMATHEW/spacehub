@model List<Booking>
@{
    ViewData["Title"] = "Booking";
}

<div class="container">
<div class="row pt-4 pb-3">

    <div class="col-6">
            <h3 class="text-primary">Booking List</h3>
    </div>
    <div class="col-6 text-end">

            <a asp-area="Admin" asp-controller="Booking" asp-action="Upsert" class="btn btn-primary">  <i class="bi bi-plus-circle pe-1"></i> New Booking</a>

    </div>
</div>


<table id="bookingTable"class="table table-bordered table-striped">
   <thead>
    <tr>
        <th>Booking ID</th>
        <th>Date</th>
        <th>User</th>
        <th>Workspace</th>
        
        

    </tr>
        </thead>
       
   
</table>

</div>
@section Scripts{
<script src="~/js/booking.js"></script>
}