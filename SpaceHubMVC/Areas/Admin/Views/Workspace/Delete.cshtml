@model Workspace
@{
    ViewData["title"] = "Delete cubicle";
}

<form method="post" class="pt-2 ">
    <input asp-for="Id" hidden />
    <div class="border p-3 mt-4">
        <div class="row pb-1">
            <h4 class="text-primary ">Delete Workspace</h4>
            <hr />
        </div>
        <div class="mb-1 row p-1">
            <label asp-for="Id"></label>
            <input asp-for="Id" disabled class="form-control p-1 " />
        </div>
        <div class="mb-1 row p-1">
            <label asp-for="Building"></label>
            <input asp-for="Building" disabled class="form-control p-1" />
        </div>
        <div class="mb-1 row p-1">
            <label asp-for="Module"></label>
            <input asp-for="Module" disabled class="form-control p-1 " />
        </div>
        <div class="mb-1 row p-1">
            <label asp-for="SeatNo"></label>
            <input asp-for="SeatNo" disabled class="form-control p-1" />
        </div>
       
        <div class="row mt-3">
            <div class="col-6 col-md-3">
                <button type="submit" class="btn btn-danger form-control">delete</button>
            </div>
            <div class="col-6 col-md-3">
                <a asp-controller="workspace" asp-action="Index" class="btn btn-secondary border form-control">Back to List</a>

            </div>
        </div>


    </div>
</form>
